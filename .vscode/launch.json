{
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceRoot}/python"
        },
        {
            "name": "JLink Debug",
            "type": "cortex-debug",
            "servertype": "jlink",
            "request": "launch",
            "executable": "${workspaceRoot}/build/NucleoTest123.elf", // byt till projektnamn p√• elf
            "device": "STM32F429ZI",
            "interface": "swd",
            "serverArgs": [
                // "-gui"
            ],
            "cwd": "${fileDirname}",
        }
    ],
    "version": "2.0.0"
}